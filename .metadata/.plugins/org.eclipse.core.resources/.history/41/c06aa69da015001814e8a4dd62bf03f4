package eg;

public class example1 {

	public static void main(String[] args) {
		
		System.out.println(fibonacci(0));

	}

public static int fibonacci(int input) {
		
		
		int case1 = 2;
		int case2 = 3;
		int caseAdd;
			caseAdd = case1 + case2;
			case1 = case2;
			case2 = caseAdd;
			System.out.println(caseAdd);
		if (input < 18) fibonacci (caseAdd + input);
		return caseAdd;
		
	}
}

