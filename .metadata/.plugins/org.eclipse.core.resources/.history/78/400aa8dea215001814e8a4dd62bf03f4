package eg;

public class example1 {

	public static void main(String[] args) {
		
		System.out.println(fibonacci(3));

	}

public static int fibonacci(int input) {
		
	if(input == 0)
        return 0;
    else if(input == 1)
      return 1;
   else
      return fibonacci(input - 1) + fibonacci(input - 2);
	}
}

