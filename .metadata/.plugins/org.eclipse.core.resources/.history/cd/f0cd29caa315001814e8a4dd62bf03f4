package eg;

public class example1 {

	public static void main(String[] args) {
		
		for (int i=0; i<14; i++) {
		System.out.println(fibonacci(i));
		}
	}

public static int fibonacci(int input) {
		
	if(input == 0)
        return 0;
    else if(input == 1)
      return 1;
   else
      return fibonacci(input - 1) + fibonacci(input - 2);
	}
}

